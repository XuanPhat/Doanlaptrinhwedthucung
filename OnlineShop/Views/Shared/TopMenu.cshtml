@model List<Model.EF.Menu>
<div class="call">
    <p><span>Need help?</span> call us <span class="number">1-22-3456789</span></span></p>
</div>
<div class="account_desc">
    @{
        var session = (UserLogin)Session[OnlineShop.Common.CommonConstants.USER_SESSION];
    }

    @if (session == null)
    {
    <ul>
        @foreach (var item in Model)
        {
        <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
        }
    </ul>
    }
    else
    {
        <ul>
            <li>Xin chào: @session.UserName</li>
            <li>@Html.ActionLink("Logout","Logout","User")</li>
            <li>@Html.ActionLink("Register", "Register", "User")</li>
        </ul>
    }



